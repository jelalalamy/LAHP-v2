{
  "manifest_version": 2,
  "name": "LAHP v2",
  "description": "A nicer new tab page for Chrome.",
  "version": "2.1",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}